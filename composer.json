{
    "name": "ironedge/cli",
    "description": "This component provides a front controller for your CLI commands, adds wizard capabilities, etc.",
    "keywords": ["cli", "menu", "wizard", "frenzy", "framework"],
    "homepage": "http://www.ironedgesoftware.com/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Falco",
            "email": "comfortablynumb84@gmail.com",
            "homepage": "http://www.ironedgesoftware.com/"
        }
    ],
    "support": {
        "issues": "https://github.com/ironedgesoftware/cli/issues"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/console": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5 || ^5.0.5",
        "phpunit/phpunit-mock-objects": "2.3.0 || ^3.0"
    },
    "_": "phpunit/phpunit-mock-objects required in 2.3.0 due to https://github.com/sebastianbergmann/phpunit-mock-objects/issues/223 - needs hhvm 3.8+ on travis",
    "autoload": {
        "psr-4": { 
            "IronEdge\\Component\\Cli\\": "src",
            "IronEdge\\Component\\Cli\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
