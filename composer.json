{
    "name": "ironedge/cliched",
    "description": "Cliched is a PHP CLI Menu / Wizard generator. Add a cliched.json to your project and encapsulate all your CLI task in a simple and powerful menu. ",
    "keywords": ["cli", "menu", "wizard"],
    "homepage": "http://www.ironedge.com/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Falco",
            "email": "comfortablynumb84@gmail.com",
            "homepage": "http://www.ironedge.com/"
        }
    ],
    "support": {
        "issues": "https://github.com/ironedgesoftware/cliched/issues"
    },
    "require": {
        "php": "^5.3.2 || ^7.0",
        "justinrainbow/json-schema": "^1.4.4",
        "seld/jsonlint": "^1.4",
        "symfony/console": "^2.5 || ^3.0",
        "symfony/finder": "^2.2 || ^3.0",
        "symfony/process": "^2.1 || ^3.0",
        "symfony/filesystem": "^2.5 || ^3.0",
        "seld/phar-utils": "^1.0",
        "seld/cli-prompt": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5 || ^5.0.5",
        "phpunit/phpunit-mock-objects": "2.3.0 || ^3.0"
    },
    "_": "phpunit/phpunit-mock-objects required in 2.3.0 due to https://github.com/sebastianbergmann/phpunit-mock-objects/issues/223 - needs hhvm 3.8+ on travis",
    "autoload": {
        "psr-4": { "IronEdge\\Components\\Cliched\\": "src/Cliched" }
    },
    "autoload-dev": {
        "psr-4": { "IronEdge\\Components\\Cliched\\Test\\": "tests/Cliched/Test" }
    },
    "extra": {
        "iron-edge": {
            "version":          "0.1.0"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}